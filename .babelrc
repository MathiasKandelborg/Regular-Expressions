{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [">1%", "last 4 versions", "Firefox ESR"]
        }
      }
    ],
    "react",
    "stage-2"
  ],
  "plugins": [
    "transform-react-inline-elements",
    "transform-react-constant-elements"
  ],
  "ignore": ["node_modules", "build", "dist"]
}
